{
	"processors": {
		"fragments": [
			{"engine": "freemarker"}
		],
		"scripts": [
			{"engine": "closure", "compilationLevel": "ADVANCED_OPTIMIZATIONS"}
		],
		"styles": [
			{"engine": "png2datauri", "imageMaxKb": 30},
			{"engine": "sass", "outputStyle": "compressed"}
		]
	},
	"buildLanguages": ["en", "es"],
	"buildDirectory": "/var/www/${appname}/httpdocs",
	"hoursUntilExpiration": 0,
	"hoursUntilDeletion": 0,
	"deploymentDestinations": {
		"devserver1": "brick@123.45.67.890/var/www/${appname}",
		"devserver2": "brick@123.45.67.891/var/www/${appname}"
	}
}